<%= form_for(@picture) do |f| %>
  <%= f.error_messages! %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
<%= link_to "ブログ一覧にもどる", blogs_path %>
