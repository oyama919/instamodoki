<%= form_for(@picture) do |f| %>
  <%= f.error_messages! %>
  <%= f.label :タイトル %><br>
  <%= f.text_field :title %>
  <%= f.file_field :image %><br>
  <%= f.hidden_field :image_cache %>
  <%= f.label :記事内容 %><br>
  <%= f.text_field :content %><br>
  <%= f.submit %>
<% end %>
<%= link_to "一覧にもどる", pictures_path %>
